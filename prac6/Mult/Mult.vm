// Calculates a = x*y
// a is a local variable
// x & y are static variables

// Put your code here.
push constant 0
pop local 0 // a = 0

push static 0
pop temp 0 // temp 0 = x

push static 1
pop temp 1 // temp 1 = y

push temp 1
push constant 0
eq
if-goto END

label MULT_LOOP

push temp 1
push constant 1
sub
pop temp 1 // y = y - 1

push temp 0    // x
push local 0   // a
add
pop local 0    // a = a + x

push temp 1
push constant 0
gt
if-goto MULT_LOOP

label END
goto END
